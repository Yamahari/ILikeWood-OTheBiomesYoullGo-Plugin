{
  "multipart": [
    {
      "when": {
        "axis": "y"
      },
      "apply": {
        "model": "ilikewood:block/post/lament"
      }
    },
    {
      "when": {
        "axis": "x"
      },
      "apply": {
        "model": "ilikewood:block/post/lament",
        "x": 90,
        "y": 90
      }
    },
    {
      "when": {
        "axis": "z"
      },
      "apply": {
        "model": "ilikewood:block/post/lament",
        "x": 90
      }
    },
    {
      "when": {
        "down": "true"
      },
      "apply": {
        "model": "ilikewood:block/post/side/lament",
        "x": 90
      }
    },
    {
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "ilikewood:block/post/side/lament",
        "x": 270
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "ilikewood:block/post/side/lament"
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "ilikewood:block/post/side/lament",
        "y": 180
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "ilikewood:block/post/side/lament",
        "y": 270
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "ilikewood:block/post/side/lament",
        "y": 90
      }
    }
  ]
}